{
  "version": 2,
  "buildCommand": "cd client && npm install && npm run build",
  "outputDirectory": "client/build",
  "installCommand": "npm install && cd client && npm install",
  "functions": {
    "api/data.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/data",
      "destination": "/api/data.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
